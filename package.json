{
	"name": "@rodrigodagostino/svelte-sortable-list",
	"version": "1.1.6",
	"author": "Rodrigo Dâ€™Agostino <rodrigo.dagostino@proton.me>",
	"keywords": [
		"sort",
		"sortable",
		"list",
		"drag",
		"drop",
		"dnd",
		"reorder",
		"reorderable",
		"accessible",
		"svelte",
		"sveltekit"
	],
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rodrigodagostino/svelte-sortable-list.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "playwright test",
		"test:debug": "playwright test --debug",
		"test:headed": "playwright test --headed",
		"test:ui": "playwright test --ui",
		"test:trace": "playwright test --trace on",
		"test:trigger-flaky": "playwright test --repeat-each=20 --workers=4 -x"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./*.css": {
			"import": "./dist/*.css"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0",
		"esm-env": "^1.0.0"
	},
	"devDependencies": {
		"@playwright/test": "1.55.0",
		"@sveltejs/adapter-auto": "6.1.0",
		"@sveltejs/kit": "2.36.1",
		"@sveltejs/package": "2.5.0",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@types/eslint": "9.6.1",
		"autoprefixer": "10.4.21",
		"eslint": "9.33.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.4",
		"eslint-plugin-svelte": "3.11.0",
		"globals": "16.3.0",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"prettier-plugin-tailwindcss": "0.6.14",
		"publint": "0.3.12",
		"svelte": "4.2.20",
		"svelte-check": "4.3.1",
		"tailwindcss": "3.4.17",
		"typescript": "5.9.2",
		"typescript-eslint": "8.40.0",
		"vite": "5.4.19"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
