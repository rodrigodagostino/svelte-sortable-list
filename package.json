		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./*.css": {
			"import": "./dist/*.css"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0",
		"esm-env": "^1.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "6.0.1",
		"@sveltejs/kit": "2.22.5",
		"@sveltejs/package": "2.3.12",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@types/eslint": "9.6.1",
		"autoprefixer": "10.4.21",
