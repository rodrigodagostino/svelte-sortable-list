{
	"name": "@rodrigodagostino/svelte-sortable-list",
	"version": "2.1.11",
	"author": "Rodrigo Dâ€™Agostino <rodrigo.dagostino@proton.me>",
	"keywords": [
		"sort",
		"sortable",
		"list",
		"drag",
		"drop",
		"dnd",
		"reorder",
		"reorderable",
		"accessible",
		"svelte",
		"sveltekit"
	],
	"license": "MIT",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rodrigodagostino/svelte-sortable-list.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "playwright test",
		"test:debug": "playwright test --debug",
		"test:headed": "playwright test --headed",
		"test:ui": "playwright test --ui",
		"test:trace": "playwright test --trace on",
		"test:trigger-flaky": "playwright test --repeat-each=20 --workers=4 -x"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./*.css": {
			"import": "./dist/*.css"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"esm-env": "^1.0.0",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "2.0.2",
		"@eslint/js": "9.39.2",
		"@fontsource/jetbrains-mono": "5.2.8",
		"@fontsource/rubik": "5.2.8",
		"@playwright/test": "1.58.2",
		"@sveltejs/adapter-auto": "7.0.1",
		"@sveltejs/kit": "2.52.0",
		"@sveltejs/package": "2.5.7",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@tailwindcss/postcss": "4.2.0",
		"@tailwindcss/vite": "4.2.0",
		"@types/eslint": "9.6.1",
		"autoprefixer": "10.4.24",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-prettier": "5.5.5",
		"eslint-plugin-svelte": "3.15.0",
		"globals": "17.3.0",
		"postcss": "8.5.6",
		"prettier": "3.8.1",
		"prettier-plugin-svelte": "3.4.1",
		"prettier-plugin-tailwindcss": "0.7.2",
		"publint": "0.3.17",
		"svelte": "5.51.4",
		"svelte-check": "4.4.1",
		"tailwindcss": "4.2.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.56.0",
		"vite": "7.3.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
