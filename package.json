		"prettier-plugin-tailwindcss": "0.6.14",
		"publint": "0.3.12",
		"svelte": "4.2.20",
		"svelte-check": "4.2.2",
		"tailwindcss": "3.4.17",
		"typescript": "5.8.3",
		"typescript-eslint": "8.36.0",
		"vite": "5.4.19"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
